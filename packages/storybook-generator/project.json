{
  "name": "storybook-generator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/storybook-generator/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/storybook-generator",
        "main": "packages/storybook-generator/src/index.ts",
        "tsConfig": "packages/storybook-generator/tsconfig.lib.json",
        "assets": [
          "packages/storybook-generator/*.md",
          {
            "input": "./packages/storybook-generator/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/storybook-generator/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/storybook-generator",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/storybook-generator",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/storybook-generator/**/*.ts", "packages/storybook-generator/generators.json", "packages/storybook-generator/executors.json", "packages/storybook-generator/package.json"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/storybook-generator"],
      "options": {
        "jestConfig": "packages/storybook-generator/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
