    "ng-packagr": "^14.2.0",
    "os-browserify": "^0.3.0",
    "rxjs": "~7.5.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "tslib": "^2.4.0",
    "tty-browserify": "^0.0.1",
