    "@nrwl/nx-plugin": "15.0.13",
    "@nrwl/storybook": "15.0.13",
    "@storybook/addon-controls": "~6.5.9",
    "@storybook/addon-essentials": "~6.5.9",
    "@storybook/addons": "^6.5.12",
